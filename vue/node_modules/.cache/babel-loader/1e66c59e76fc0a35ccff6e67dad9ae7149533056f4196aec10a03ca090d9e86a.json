{"ast":null,"code":"export default {\n  name: \"ApplicationPending\",\n  data() {\n    return {\n      available: false,\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      orderForm: {},\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      orderFormDate: [],\n      // 查询到的所有信息都存在\n      textbookName: null\n    };\n  },\n  created() {\n    // this.load(1)\n    // this.loadOrderForm()\n  },\n  methods: {\n    loadOrderForm() {\n      this.$request.get('/orderForm/selectAll').then(res => {\n        if (res.code === '200') {\n          this.orderFormDate = res.data;\n        } else {\n          this.$message.error(res, msg);\n        }\n      });\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/orderForm/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          textbookName: this.textbookName\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    cancelOrder(row) {\n      // 取消征订单时改为2 已取消\n      this.form = JSON.parse(JSON.stringify(row));\n      this.form.cancelStatus = '2';\n      // 向后端发起请求修改 征订单是否取消的状态\n      this.$request.put('/orderForm/cancelStatus', this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success('修改成功');\n          this.load(1);\n        } else {\n          this.$message.error(res.msg); // 弹出错误的信息\n        }\n      });\n    },\n\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          this.$request({\n            // 是否存在id判断新增还是修改的关键\n            url: this.form.textbookId ? '/textbook/updateTextbook' : '/textbook/addTextbook',\n            method: this.form.textbookId ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 表示成功保存\n              this.$message.success('保存成功');\n              this.load(1);\n              this.fromVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    saveOrder() {\n      // 生成征订单\n      this.$refs['orderFormRef'].validate(valid => {\n        if (valid) {\n          this.$request.post(\"/orderForm/addOrderForm\", this.orderForm).then(res => {\n            if (res.code === '200') {\n              this.$message.success('征订成功');\n              this.orderVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    reset() {\n      this.textbookName = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    // 上传文件的方法\n    handleBookCoverSuccess(response, file, fileList) {\n      // 把头像属性换成上传的图片的链接\n      this.form.bookCover = response.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","available","tableData","pageNum","pageSize","total","orderForm","form","user","JSON","parse","localStorage","getItem","orderFormDate","textbookName","created","methods","loadOrderForm","$request","get","then","res","code","$message","error","msg","load","params","list","cancelOrder","row","stringify","cancelStatus","put","success","save","$refs","validate","valid","url","textbookId","method","fromVisible","saveOrder","post","orderVisible","reset","handleCurrentChange","handleBookCoverSuccess","response","file","fileList","bookCover"],"sources":["src/views/manager/ApplicationPending.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入教材名称\" style=\"width: 200px\"\r\n                v-model=\"textbookName\">\r\n      </el-input>\r\n\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\">\r\n\r\n      <el-button style=\"margin-left: 10px\" v-if=\"user.roleName ==='ADMIN' \" type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n      <el-button v-if=\"user.roleName ==='ADMIN' \" type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n      <el-button v-if=\"user.roleName ==='ADMIN' \" type=\"success\" plain @click=\"eport\">导入书单</el-button>\r\n\r\n      <el-switch style=\"margin-left: 10px\"\r\n                 v-model=\"available\"\r\n                 active-text=\"只看\"\r\n                 @change=\"loadOrderStatus\">\r\n      </el-switch>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe  @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\"idth=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"orderFormId\" label=\"序号\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"isbn\" label=\"ISBN号\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"textbookName\" label=\"教材名称\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"applicationNumber\" label=\"申请数量\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"departmentName\" label=\"使用部门\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"applicationTime\" label=\"申请时间\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"realName\" label=\"申请人\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"applicationStatus\" label=\"审核状态\" align=\"center\">\r\n          <template v-slot=\"{ row }\">\r\n            <span v-if=\"row.applicationStatus === '1'\">未审批</span>\r\n            <span v-else-if=\"row.applicationStatus === '2'\" >待教务处审批</span>\r\n            <span v-else-if=\"row.applicationStatus === '3'\" >已通过</span>\r\n            <span v-else>未通过</span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column width=\"180\" label=\"操作\" align=\"center\">\r\n          <template v-slot=\"{ row }\">\r\n           <el-button v-if=\"row.cancelStatus === '1'\" type=\"primary\" plain=\"true\" size=\"small\" @click=\"cancelOrder(scope.row)\" >取消申请</el-button>\r\n           <span v-else>已取消</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--       分页-->\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[1, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ApplicationPending\",\r\n  data() {\r\n    return {\r\n      available: false,\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      orderForm: {},\r\n      form: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      orderFormDate: [], // 查询到的所有信息都存在\r\n      textbookName: null,\r\n    }\r\n  },\r\n  created() {\r\n    // this.load(1)\r\n    // this.loadOrderForm()\r\n  },\r\n  methods: {\r\n    loadOrderForm() {\r\n      this.$request.get('/orderForm/selectAll').then(res =>{\r\n        if(res.code === '200') {\r\n          this.orderFormDate = res.data\r\n        } else {\r\n          this.$message.error(res,msg)\r\n        }\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n\r\n      this.$request.get('/orderForm/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          textbookName : this.textbookName\r\n        },\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    cancelOrder(row) {\r\n      // 取消征订单时改为2 已取消\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.form.cancelStatus = '2'\r\n      // 向后端发起请求修改 征订单是否取消的状态\r\n      this.$request.put('/orderForm/cancelStatus',this.form).then(res =>{\r\n        if (res.code === '200'){\r\n          this.$message.success('修改成功')\r\n          this.load(1)\r\n        }else {\r\n          this.$message.error(res.msg)  // 弹出错误的信息\r\n        }\r\n      })\r\n\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          this.$request({ // 是否存在id判断新增还是修改的关键\r\n            url: this.form.textbookId ? '/textbook/updateTextbook' : '/textbook/addTextbook',\r\n            method: this.form.textbookId ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    saveOrder() { // 生成征订单\r\n      this.$refs['orderFormRef'].validate((valid) => {\r\n        if (valid) {\r\n          this.$request.post(\"/orderForm/addOrderForm\",this.orderForm).then(res =>{\r\n            if (res.code === '200'){\r\n              this.$message.success('征订成功')\r\n              this.orderVisible = false\r\n            }else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n\r\n    reset() {\r\n      this.textbookName = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    // 上传文件的方法\r\n    handleBookCoverSuccess(response, file, fileList) {\r\n      // 把头像属性换成上传的图片的链接\r\n      this.form.bookCover = response.data\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAuEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,SAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,aAAA;MAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACA;IACA;EAAA,CACA;EACAC,OAAA;IACAC,cAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,aAAA,GAAAQ,GAAA,CAAArB,IAAA;QACA;UACA,KAAAuB,QAAA,CAAAC,KAAA,CAAAH,GAAA,EAAAI,GAAA;QACA;MACA;IACA;IACAC,KAAAvB,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MAEA,KAAAe,QAAA,CAAAC,GAAA;QACAQ,MAAA;UACAxB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAU,YAAA,OAAAA;QACA;MACA,GAAAM,IAAA,CAAAC,GAAA;QACA,KAAAnB,SAAA,GAAAmB,GAAA,CAAArB,IAAA,EAAA4B,IAAA;QACA,KAAAvB,KAAA,GAAAgB,GAAA,CAAArB,IAAA,EAAAK,KAAA;MACA;IACA;IACAwB,YAAAC,GAAA;MACA;MACA,KAAAvB,IAAA,GAAAE,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAsB,SAAA,CAAAD,GAAA;MACA,KAAAvB,IAAA,CAAAyB,YAAA;MACA;MACA,KAAAd,QAAA,CAAAe,GAAA,iCAAA1B,IAAA,EAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAW,OAAA;UACA,KAAAR,IAAA;QACA;UACA,KAAAH,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IAEA;;IACAU,KAAA;MAAA;MACA,KAAAC,KAAA,YAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAApB,QAAA;YAAA;YACAqB,GAAA,OAAAhC,IAAA,CAAAiC,UAAA;YACAC,MAAA,OAAAlC,IAAA,CAAAiC,UAAA;YACAxC,IAAA,OAAAO;UACA,GAAAa,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cAAA;cACA,KAAAC,QAAA,CAAAW,OAAA;cACA,KAAAR,IAAA;cACA,KAAAgB,WAAA;YACA;cACA,KAAAnB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;YACA;UACA;QACA;MACA;IACA;;IACAkB,UAAA;MAAA;MACA,KAAAP,KAAA,iBAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAApB,QAAA,CAAA0B,IAAA,iCA