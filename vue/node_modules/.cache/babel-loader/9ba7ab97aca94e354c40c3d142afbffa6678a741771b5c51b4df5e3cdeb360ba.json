{"ast":null,"code":"export default {\n  name: \"Application\",\n  data() {\n    return {\n      fromVisible: false,\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      orderForm: {},\n      orderFormRules: [{\n        applicationStatus: [{\n          required: true,\n          message: '请选择',\n          trigger: 'blur'\n        }]\n      }],\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      orderFormDate: [] // 查询到的所有信息都存在\n    };\n  },\n\n  created() {\n    this.load(1);\n    this.loadOrderForm();\n  },\n  methods: {\n    loadOrderForm() {\n      this.$request.get('/orderForm/selectAll').then(res => {\n        if (res.code === '200') {\n          this.orderFormDate = res.data;\n        } else {\n          this.$message.error(res, msg);\n        }\n      });\n    },\n    load(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/orderForm/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          textbookName: this.textbookName,\n          cancelStatus: this.cancelStatus\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        // 系主任和教务处要动态显示\n        if (this.user.roleName === 'MANAGER') {\n          // 如果是系主任  那就显示征订单状态\n          this.tableData = this.tableData.filter(o => o.applicationStatus === '1');\n        } else {\n          // 再不济就是管理员\n          this.tableData = this.tableData.filter(o => o.applicationStatus === '2');\n        }\n        this.total = res.data?.total;\n      });\n    },\n    approve() {\n      // 把那行的东西给到弹框\n      this.orderForm = JSON.parse(JSON.stringify(row));\n      // // 把审批状态置空吧\n      // this.orderForm.applicationStatus = null\n      this.orderForm = {};\n      this.fromVisible = true;\n    },\n    saveOrderForm() {\n      this.$request.post('/orderForm/approveOrder', this.orderForm, {\n        params: {\n          roleId: this.user.roleId // 需要判断一下是谁审批的\n        }\n      }).then(res => {\n        if (code === '200') {\n          this.$message.success('操作成功');\n          this.fromVisible = false;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    reset() {\n      this.textbookName = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    // 上传文件的方法\n    handleBookCoverSuccess(response, file, fileList) {\n      // 把头像属性换成上传的图片的链接\n      this.form.bookCover = response.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","fromVisible","tableData","pageNum","pageSize","total","orderForm","orderFormRules","applicationStatus","required","message","trigger","user","JSON","parse","localStorage","getItem","orderFormDate","created","load","loadOrderForm","methods","$request","get","then","res","code","$message","error","msg","params","textbookName","cancelStatus","list","roleName","filter","o","approve","stringify","row","saveOrderForm","post","roleId","success","reset","handleCurrentChange","handleBookCoverSuccess","response","file","fileList","form","bookCover"],"sources":["src/views/manager/Application.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"请输入教材名称\" style=\"width: 200px\"\r\n                v-model=\"textbookName\">\r\n      </el-input>\r\n\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">查询</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div class=\"operation\">\r\n\r\n      <el-button style=\"margin-left: 10px\" v-if=\"user.roleName ==='ADMIN' \" type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n      <el-button v-if=\"user.roleName ==='ADMIN' \" type=\"danger\" plain @click=\"delBatch\">批量删除</el-button>\r\n      <el-button v-if=\"user.roleName ==='ADMIN' \" type=\"success\" plain @click=\"eport\">导入书单</el-button>\r\n\r\n      <el-button>\r\n        <el-switch\r\n            style=\"margin-left: 10px\"\r\n            v-model=\"noCancel\"\r\n            active-text=\"未审批\"\r\n            @change=\"loadOrderStatus\">\r\n        </el-switch>\r\n      </el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe  @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\"idth=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"orderFormId\" label=\"序号\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"isbn\" label=\"ISBN号\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"textbookName\" label=\"教材名称\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"applicationNumber\" label=\"申请数量\" show-overflow-tooltip align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"departmentName\" label=\"使用部门\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"applicationTime\" label=\"申请时间\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"realName\" label=\"申请人\" align=\"center\"></el-table-column>\r\n\r\n        <el-table-column width=\"180\" label=\"操作\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-button v-if=\"scope.row.applicationStatus === '1'\" type=\"primary\" plain=\"true\" size=\"small\"\r\n                       @click=\"approve\" >审批</el-button>\r\n            <span v-else>已审批</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!--       分页-->\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[1, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n<!--      通过与否的弹窗-->\r\n      <el-dialog title=\"审批\" name=\"信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n        <el-form :model=\"orderForm\" :rules=\"orderFormRules\" ref=\"orderFormRef\" label-width=\"100px\" style=\"padding-right: 50px;\">\r\n\r\n          <el-form-item label=\"审批状态\" prop=\"applicationStatus\">\r\n            <el-select v-model=\"orderForm.applicationStatus\" style=\"width: 100%\" placeholder=\"请选择\">\r\n              <el-option label=\"通过\" value=\"2\"></el-option>\r\n              <el-option label=\"不通过\" value=\"4\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"审批回复\" prop=\"replyMessage\">\r\n           <el-input type=\"textarea\" v-model=\"orderForm.replyMessage\" placeholder=\"审批回复\"></el-input>\r\n          </el-form-item>\r\n          <div style=\"text-align: center; margin-bottom: 20px; margin-left: 20px\">\r\n            <el-button type=\"primary\" @click=\"saveOrderForm\">保 存</el-button>\r\n          </div>\r\n        </el-form>\r\n      </el-dialog>\r\n\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"Application\",\r\n  data() {\r\n    return {\r\n      fromVisible: false,\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      orderForm: {},\r\n      orderFormRules: [{\r\n        applicationStatus: [{\r\n            required: true, message: '请选择', trigger: 'blur',\r\n        }]\r\n      }\r\n      ],\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      orderFormDate: [], // 查询到的所有信息都存在\r\n\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n    this.loadOrderForm()\r\n  },\r\n  methods: {\r\n    loadOrderForm() {\r\n      this.$request.get('/orderForm/selectAll').then(res =>{\r\n        if(res.code === '200') {\r\n          this.orderFormDate = res.data\r\n        } else {\r\n          this.$message.error(res,msg)\r\n        }\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n\r\n      this.$request.get('/orderForm/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          textbookName : this.textbookName,\r\n          cancelStatus : this.cancelStatus\r\n        },\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        // 系主任和教务处要动态显示\r\n        if (this.user.roleName === 'MANAGER'){\r\n          // 如果是系主任  那就显示征订单状态\r\n          this.tableData = this.tableData.filter(o => o.applicationStatus === '1')\r\n        }else {\r\n          // 再不济就是管理员\r\n          this.tableData = this.tableData.filter(o => o.applicationStatus === '2')\r\n        }\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    approve(){\r\n      // 把那行的东西给到弹框\r\n      this.orderForm = JSON.parse(JSON.stringify(row))\r\n      // // 把审批状态置空吧\r\n      // this.orderForm.applicationStatus = null\r\n      this.orderForm = {}\r\n      this.fromVisible = true\r\n\r\n    },\r\n    saveOrderForm() {\r\n      this.$request.post('/orderForm/approveOrder',this.orderForm, {\r\n        params: {\r\n          roleId: this.user.roleId // 需要判断一下是谁审批的\r\n        }\r\n      }).then(res => {\r\n        if (code === '200'){\r\n          this.$message.success('操作成功')\r\n          this.fromVisible = false\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    reset() {\r\n      this.textbookName = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    // 上传文件的方法\r\n    handleBookCoverSuccess(response, file, fileList) {\r\n      // 把头像属性换成上传的图片的链接\r\n      this.form.bookCover = response.data\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAuFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,SAAA;MACAC,cAAA;QACAC,iBAAA;UACAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QACA;MACA,EACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,aAAA;IAEA;EACA;;EACAC,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAD,cAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,aAAA,GAAAQ,GAAA,CAAAzB,IAAA;QACA;UACA,KAAA2B,QAAA,CAAAC,KAAA,CAAAH,GAAA,EAAAI,GAAA;QACA;MACA;IACA;IACAV,KAAAhB,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MAEA,KAAAmB,QAAA,CAAAC,GAAA;QACAO,MAAA;UACA3B,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACA2B,YAAA,OAAAA,YAAA;UACAC,YAAA,OAAAA;QACA;MACA,GAAAR,IAAA,CAAAC,GAAA;QACA,KAAAvB,SAAA,GAAAuB,GAAA,CAAAzB,IAAA,EAAAiC,IAAA;QACA;QACA,SAAArB,IAAA,CAAAsB,QAAA;UACA;UACA,KAAAhC,SAAA,QAAAA,SAAA,CAAAiC,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAA5B,iBAAA;QACA;UACA;UACA,KAAAN,SAAA,QAAAA,SAAA,CAAAiC,MAAA,CAAAC,CAAA,IAAAA,CAAA,CAAA5B,iBAAA;QACA;QACA,KAAAH,KAAA,GAAAoB,GAAA,CAAAzB,IAAA,EAAAK,KAAA;MACA;IACA;IACAgC,QAAA;MACA;MACA,KAAA/B,SAAA,GAAAO,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAyB,SAAA,CAAAC,GAAA;MACA;MACA;MACA,KAAAjC,SAAA;MACA,KAAAL,WAAA;IAEA;IACAuC,cAAA;MACA,KAAAlB,QAAA,CAAAmB,IAAA,iCAAAnC,SAAA;QACAwB,MAAA;UACAY,MAAA,OAAA9B,IAAA,CAAA8B,MAAA;QACA;MACA,GAAAlB,IAAA,CAAAC,GAAA;QACA,IAAAC,IAAA;UACA,KAAAC,QAAA,CAAAgB,OAAA;UACA,KAAA1C,WAAA;QACA;UACA,KAAA0B,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IAEAe,MAAA;MACA,KAAAb,YAAA;MACA,KAAAZ,IAAA;IACA;IACA0B,oBAAA1C,OAAA;MACA,KAAAgB,IAAA,CAAAhB,OAAA;IACA;IACA;IACA2C,uBAAAC,QAAA,EAAAC,IAAA,EAAAC,QAAA;MACA;MACA,KAAAC,IAAA,CAAAC,SAAA,GAAAJ,QAAA,CAAA/C,IAAA;IACA;EAEA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}